{
  "name": "sapphire",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "dev": "vite --open",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "serve": "vite preview",
    "eslint": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --ignore-path ./.gitignore",
    "tslint": "tsc --noEmit",
    "prettier": "prettier ./src --loglevel warn \"./src/**/*.{ts,tsx}\" --check",
    "eslint:fix": "yarn lint:fix --fix",
    "prettier:fix": "yarn prettier --write",
    "lint": "yarn prettier && yarn eslint && yarn tslint",
    "lint:fix": "yarn prettier:fix && yarn eslint:prettier",
    "check": "concurrently \"yarn test\" \"yarn eslint\" \"yarn tslint\" \"yarn prettier\"",
    "test": "vitest",
    "release": "release-it"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "axios": "^1.5.0",
    "framer-motion": "^10.16.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-react": "^4.0.4",
    "commitlint": "^17.7.1",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "jest-environment-jsdom": "^29.6.4",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "msw": "^1.2.5",
    "prettier": "^3.0.3",
    "release-it": "^16.1.5",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.34.3"
  },
  "packageManager": "yarn@3.6.3",
  "resolutions": {
    "typescript": "5.1.6",
    "vite-plugin-checker/meow": "8.1.2"
  }
}
